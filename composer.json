{
    "name": "magicoli/reclaim-details",
    "description": "Take back control of your WordPress plugin's details display, using local readme.txt and assets",
    "version": "1.0.0",
    "type": "library",
    "keywords": ["wordpress", "plugin", "details", "readme", "independent"],
    "homepage": "https://github.com/magicoli/reclaim-details",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Magiiic",
            "email": "olivier@van-helden.net",
            "homepage": "https://magiiic.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "capevace/wp-package-parser": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Reclaim\\Details\\": "src/Reclaim/Details/"
        }
    },
    "support": {
        "issues": "https://github.com/magicoli/reclaim-details/issues",
        "source": "https://github.com/magicoli/reclaim-details"
    },
    "minimum-stability": "stable"
